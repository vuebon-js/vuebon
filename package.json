    "mini-css-extract-plugin": "^1.0.0",
    "node-sass": "^4.14.1",
    "postcss": "^8.1.9",
    "postcss-loader": "^4.0.4",
    "sass-loader": "^10.0.3",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^4.2.3",
    "ts-loader": "^8.0.7",
    "typescript": "^4.0.3",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.12",
